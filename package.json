{
  "name": "edabit-js-challenges",
  "version": "1.0.0",
  "description": "Solutions for JavaScript challenges on Edabit",
  "private": true,
  "scripts": {
    "format": "pretty-quick",
    "format:staged": "pretty-quick --staged",
    "lint": "eslint \"src/**/*.js\"",
    "test": "echo \"test\"",
    "test:watch": "echo \"test:watch\"",
    "custom": "sh scripts/checkDuplicateLinks.sh && sh scripts/checkFolderNames.sh",
    "all": "npm run format:staged && npm run custom && npm run lint && npm t"
  },
  "author": {
    "name": "Loris Bettazza",
    "email": "loris.bettazza@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Pustur/edabit-js-challenges.git"
  },
  "devDependencies": {
    "eslint": "6.1.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.1",
    "husky": "3.0.5",
    "jest": "24.9.0",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run all"
    }
  },
  "license": "MIT"
}
